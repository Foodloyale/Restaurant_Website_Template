}

<div id="carouselExampleControlsNoTouching" class="carousel slide" data-bs-touch="true" style="width:80%">
    <div class="carousel-inner">
        <div class="carousel-item active">
            <img src="~/images/AdobeStock_243652645-scaled.jpeg" class="d-block w-100" alt="..." />
        </div>
        <div class="carousel-item">
            <img src="~/images/serving-food.jpg" class="d-block w-100" alt="..."/>
